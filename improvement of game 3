    <script type="text/javascript">
        var music =document.getElementById('audio1');
        music.volume = 0.2;
        var isPlaying = false;
         var ReadyToPlay = document.getElementById('ReadyToPlay');
         var SelectChar = document.getElementById('SelectChar');
         var Loading = document.getElementById('Loading');
         var playGame = document.getElementById('play');
         var v1 = document.getElementById('1');
         var v2 = document.getElementById('2');
         var v3 = document.getElementById('3');
         var q1 = document.getElementById('q1');
         var q1button = document.getElementById('q1button');
         var q1abandon = document.getElementById('q1abandon');
         var q1respect = document.getElementById('q1respect');
         var q1a1 = document.getElementById('q1a1');
         var q1a2 = document.getElementById('q1a2');
         var q1res = document.getElementById('q1res');
         var q1reselectButton = document.getElementById('q1reselectButton');
         var q1select = document.getElementById('q1select');
         var choosetoy = document.getElementById('choosetoy');
         var bear = document.getElementById('bear');
         var dragon = document.getElementById('dragon');
         var choosetoyButton = document.getElementById('choosetoyButton');
         var selectbear = document.getElementById('selectbear');
        var selectbear = document.getElementById('selectdragon');
         var aftershopping = document.getElementById('aftershopping');
         var backtohome = document.getElementById('backtohome');
         var toyselect = document.getElementById('toyselect');
         var rob = document.getElementById('rob');
         var exchange = document.getElementById('exchange');
         var robtoy = document.getElementById('robtoy');
         var toyreselect = document.getElementById('toyreselect');
         var toyreselection = document.getElementById('toyreselection');
         var exchangetoy = document.getElementById('exchangetoy');
         var losegame = document.getElementById('losegame');
         var throwdesk = document.getElementById('throwdesk');
         var losegamereselection = document.getElementById('losegamereselection');
         var reselectlosegame = document.getElementById('reselectlosegame');
         var chooseChar = document.getElementById('chooseChar');
         var selectelder = document.getElementById('selecteleder');
         var selectyounger = document.getElementById('selectyounger');
         var selectChar = document.getElementById('SelectChar');
        
        
        
        document.getElementById('go_home').addEventListener('click',function(e){
                    window.location.href = 'http://www.savetheyouth-iteration3.tk/games/';
            },false)
            music.volume = 0.2;
            document.getElementById('volume').addEventListener('click',function(e){
                if(isPlaying){
                    music.pause();
                    document.getElementById('off').setAttribute('class',"fa fa-volume-off");
                }
                else{
                    music.play();
                    document.getElementById('off').setAttribute('class',"fa fa-volume-up");
                }
            },false)
            music.onplaying =function(){
                isPlaying = true;
            }
            music.onpause =function(){
                isPlaying = false;
            }
        
        document.getElementById('continue1').addEventListener('click',function(e){
                    document.getElementById('warning').style.display = 'none';
                    document.getElementById('exit').style.display = 'none';
                    document.getElementById('continue').style.display = 'none';
                    document.getElementById('ReadyToPlay').style.display ='block';
                    document.getElementById('play').style.display ='block';
                    document.getElementById('ReadyToPlay').play();
            },false)

         document.getElementById('exit').addEventListener('click',function(e){
                    window.location.href = 'http://www.savetheyouth-iteration3.tk/games/';
            },false)
        
        
        
        
        
        
        playGame.addEventListener("click", function(event) {
        ReadyToPlay.style.display = 'none' ;
        Loading.style.display = 'block' ;
            Loading.play();
            playGame.style.display= 'none'
}, false);
        
        Loading.onended = function(){
            Loading.style.display = 'none';
            selectChar.style.display = 'block';
            selectChar.play();
            chooseChar.style.display ='block';
//            v1.style.display = 'block';
//            v1.play();
            
        }
        selectelder.addEventListener('click',function(event){
            selectChar.style.display = 'none';
            chooseChar.style.display ='none';
            v1.style.display = 'block';
            v1.play(); 
        },false)
        
        
        selectyounger.addEventListener('click',function(event){
            selectChar.style.display = 'none';
            chooseChar.style.display ='none';
